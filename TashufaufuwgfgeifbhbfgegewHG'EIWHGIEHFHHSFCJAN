ba.BackgroundTransparency=1
ba.Size=UDim2.new(1,0,1,0)
ba.Text=""
ba.Name="OptionButton"
ba.Parent=a9

bb.Font=Enum.Font.GothamBold
bb.Text=a7
bb.TextSize=13
bb.TextColor3=Color3.fromRGB(230,230,230)
bb.Position=UDim2.new(0,8,0,8)
bb.Size=UDim2.new(1,-100,0,13)
bb.BackgroundTransparency=1
bb.TextXAlignment=Enum.TextXAlignment.Left
bb.Name="OptionText"
bb.Parent=a9

a9:SetAttribute("RealValue",a8)

bc.AnchorPoint=Vector2.new(0,0.5)
bc.BackgroundColor3=ab.Color
bc.Position=UDim2.new(0,2,0.5,0)
bc.Size=UDim2.new(0,0,0,0)
bc.Name="ChooseFrame"
bc.Parent=a9

bd.Color=ab.Color
bd.Thickness=1.6
bd.Transparency=0.999
bd.Parent=bc
be.Parent=bc

ba.Activated:Connect(function()
if aP.Multi then
if not table.find(aR.Value,a8)then
table.insert(aR.Value,a8)
else
for bg,bh in pairs(aR.Value)do
if bh==a8 then
table.remove(aR.Value,bg)
break
end
end
end
else
aR.Value=a8
end
aR:Set(aR.Value)
end)
end

function aR.Set(a5,a6)
if aP.Multi then
aR.Value=type(a6)=="table"and a6 or{}
else
aR.Value=(type(a6)=="table"and a6[1])or a6
end

ConfigData[aQ]=aR.Value


local a7={}
for a8,a9 in a2:GetChildren()do
if a9.Name=="Option"and a9:FindFirstChild"OptionText"then
local ba=a9:GetAttribute"RealValue"
local bb=aP.Multi and table.find(aR.Value,ba)or
aR.Value==ba

if bb then
h:Create(a9.ChooseFrame,TweenInfo.new(0.2),
{Size=UDim2.new(0,1,0,12)}):Play()
h:Create(a9.ChooseFrame.UIStroke,TweenInfo.new(0.2),{Transparency=0})
:Play()
h:Create(a9,TweenInfo.new(0.2),{BackgroundTransparency=0.935}):Play()
table.insert(a7,a9.OptionText.Text)
else
h:Create(a9.ChooseFrame,TweenInfo.new(0.1),
{Size=UDim2.new(0,0,0,0)}):Play()
h:Create(a9.ChooseFrame.UIStroke,TweenInfo.new(0.1),
{Transparency=0.999}):Play()
h:Create(a9,TweenInfo.new(0.1),{BackgroundTransparency=0.999}):Play()
end
end
end

aZ.Text=(#a7==0)
and(aP.Multi and"Select Options"or"Select Option")
or table.concat(a7,", ")

if aP.Callback then
if aP.Multi then
aP.Callback(aR.Value)
else
local a8=(aR.Value~=nil)and tostring(aR.Value)or""
aP.Callback(a8)
end
end
end

function aR.SetValue(a5,a6)
a5:Set(a6)
end

function aR.GetValue(a5)
return a5.Value
end

function aR.SetValues(a5,a6,a7)
a6=a6 or{}
a7=a7 or(aP.Multi and{}or nil)
aR:Clear()
for a8,a9 in ipairs(a6)do
aR:AddOption(a9)
end
aR.Options=a6
aR:Set(a7)
end

aR:SetValues(aR.Options,aR.Value)

aM=aM+1
W=W+1
Elements[aQ]=aR
return aR
end

