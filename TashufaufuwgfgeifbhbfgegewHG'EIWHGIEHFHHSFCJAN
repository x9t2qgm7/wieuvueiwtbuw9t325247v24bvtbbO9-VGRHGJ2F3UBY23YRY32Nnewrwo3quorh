local function UpdateSize1()
local I=0
for J,K in G:GetChildren()do
if K.Name~="UIListLayout"then
I=I+3+K.Size.Y.Offset
end
end
G.CanvasSize=UDim2.new(0,0,0,I)
end
G.ChildAdded:Connect(UpdateSize1)
G.ChildRemoved:Connect(UpdateSize1)

