function aL.AddConfigPanel(aN)
local aO=d or""


local aP=aL:AddParagraph{
Title="Config Manager",
Content="Current: "..(d or"None").." | Autoload: "..(GetAutoload()or"None"),
Icon="settings"
}


local aQ=aL:AddInput{
Title="Config Name",
Content="Enter the name for your config",
Placeholder="e.g., Default, PVP, Farming",
Default=d or"",
Callback=function(aQ)
aO=aQ
end
}


local aR=GetConfigList()
local aS=aL:AddDropdown{
Title="Select Config",
Content="Choose from existing configs",
Options=aR,
Default=nil,
Callback=function(aS)
aO=aS
aQ:Set(aS)
end
}


aL:AddButton{
Title="Save Config",
SubTitle="Load Config",
Callback=function()
if aO~=""then
if SaveConfigAs(aO)then
chloex("Config saved: "..aO,3,Color3.fromRGB(100,255,100))

local aT=GetConfigList()
aS:SetValues(aT,aO)
aP:SetContent("Current: "..aO.." | Autoload: "..(GetAutoload()or"None"))
end
else
chloex("Please enter a config name",3,Color3.fromRGB(255,100,100))
end
end,
SubCallback=function()
if aO~=""then
if LoadConfig(aO)then
chloex("Config loaded: "..aO,3,Color3.fromRGB(100,255,100))
aP:SetContent("Current: "..aO.." | Autoload: "..(GetAutoload()or"None"))
else
chloex("Failed to load: "..aO,3,Color3.fromRGB(255,100,100))
end
else
chloex("Please select a config",3,Color3.fromRGB(255,100,100))
end
end
}


aL:AddButton{
Title="Delete Config",
SubTitle="Set Autoload",
Callback=function()
if aO~=""then

chloex("Click again to confirm delete: "..aO,4,Color3.fromRGB(255,150,100))
task.wait(0.5)

if DeleteConfig(aO)then
chloex("Config deleted: "..aO,3,Color3.fromRGB(255,100,100))
aO=""
aQ:Set""


local aT=GetConfigList()
aS:SetValues(aT,nil)
aP:SetContent("Current: "..(d or"None").." | Autoload: "..(GetAutoload()or"None"))
end
else
chloex("Please select a config to delete",3,Color3.fromRGB(255,100,100))
end
end,
SubCallback=function()
if aO~=""then
SetAutoload(aO)
chloex("Autoload set to: "..aO,3,Color3.fromRGB(100,200,255))
aP:SetContent("Current: "..(d or"None").." | Autoload: "..aO)
else
chloex("Please enter a config name",3,Color3.fromRGB(255,100,100))
end
end
}


aL:AddButton{
Title="Refresh List",
SubTitle="Clear Autoload",
Callback=function()
local aT=GetConfigList()
aS:SetValues(aT,nil)
chloex("Config list refreshed ("..#aT.." configs)",2,Color3.fromRGB(150,150,255))
end,
SubCallback=function()
ClearAutoload()
chloex("Autoload cleared",3,Color3.fromRGB(200,200,100))
aP:SetContent("Current: "..(d or"None").." | Autoload: None")
end
}


aL:AddButton{
Title="Reset All Elements to Default",
Callback=function()
ResetElements()
chloex("All elements reset to default values",3,Color3.fromRGB(255,200,100))
end
}


return{
UpdateStatus=function()
aP:SetContent("Current: "..(d or"None").." | Autoload: "..(GetAutoload()or"None"))
end,
RefreshList=function()
local aT=GetConfigList()
aS:SetValues(aT,nil)
end
}
end

ar=ar+1
return aL
end

V=V+1
local as=Z.Name:gsub("%s+","_")
_G[as]=aq
return aq
end

return U
end


if ab.Keysystem.Enabled then
local ac={
Title=ab.Keysystem.Title or"Key System",
Description=ab.Keysystem.Description or"Enter your key below",
ScriptId=ab.Keysystem.ScriptId or"",
KeyFile=ab.Keysystem.KeyFile or"NoctisPremium.txt",
Discord=ab.Keysystem.Discord,
GetKeyLinks=ab.Keysystem.GetKeyLinks or{},
LogoAsset=ab.Keysystem.LogoAsset or"rbxassetid://123156553209294"
}

local ad

CreateKeySystem(ac,function(ae,af)
if ae then

ad=CreateWindowUI()
end
end)


repeat task.wait()until ad~=nil
return ad
else

return CreateWindowUI()
end
end

return n
