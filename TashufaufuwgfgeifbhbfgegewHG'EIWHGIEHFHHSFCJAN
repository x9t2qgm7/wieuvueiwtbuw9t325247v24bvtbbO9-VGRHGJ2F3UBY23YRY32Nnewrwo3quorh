local function UpdateStatus(aa,ab)
L.Text=aa
L.TextColor3=ab
end

local aa=false

local function VerifyKey()
if aa then return end

local ab=M.Text:gsub("%s+","")
if ab==""then
UpdateStatus("Please enter a key!",Color3.fromRGB(255,100,100))
task.wait(2)
UpdateStatus(o.Description,Color3.fromRGB(255,255,255))
return
end

aa=true
UpdateStatus("Checking key...",Color3.fromRGB(255,200,100))
T.Text="Checking..."

task.spawn(function()
local ac,ad=checkKey(ab)

if ac then
UpdateStatus("Key valid! Loading...",Color3.fromRGB(100,255,100))


pcall(function()script_key=ab end)
pcall(function()_G.script_key=ab end)
pcall(function()getgenv().script_key=ab end)


if writefile then
pcall(function()
writefile(o.KeyFile,ab)
end)
end

task.wait(0.5)
w:Destroy()

if p then
task.spawn(p,true,ab)
end
else
local ae={
KEY_HWID_LOCKED="Key locked to different device",
KEY_INCORRECT="Invalid key",
KEY_EXPIRED="Key expired",
KEY_BANNED="Key banned",
KEY_INVALID="Invalid key format",
SCRIPT_ID_INCORRECT="Script ID error",
SCRIPT_ID_INVALID="Invalid script ID",
INVALID_EXECUTOR="Unsupported executor"
}

local af=ae[ad.code]or("Error: "..tostring(ad.message))
UpdateStatus(af,Color3.fromRGB(255,100,100))
T.Text="Verify Key"
aa=false
task.wait(2)
UpdateStatus(o.Description,Color3.fromRGB(255,255,255))
end
end)
end


H.MouseButton1Click:Connect(function()
w:Destroy()
if p then p(false)end
end)

Q.MouseButton1Click:Connect(function()
VerifyKey()
end)

M.FocusLost:Connect(function(ab)
if ab then
VerifyKey()
end
end)


local ab,ac,ad,ae

A.InputBegan:Connect(function(af)
if af.UserInputType==Enum.UserInputType.MouseButton1 then
ab=true
ad=af.Position
ae=x.Position

af.Changed:Connect(function()
if af.UserInputState==Enum.UserInputState.End then
ab=false
end
end)
end
end)

A.InputChanged:Connect(function(af)
if af.UserInputType==Enum.UserInputType.MouseMovement then
ac=af
end
end)

v.InputChanged:Connect(function(af)
if af==ac and ab then
local ag=af.Position-ad
x.Position=UDim2.new(ae.X.Scale,ae.X.Offset+ag.X,ae.Y.Scale,ae.Y.Offset+ag.Y)
end
end)
end

function n.Window(aa,ab)
ab=ab or{}
ab.Title=ab.Title or"Chloe X"
ab.Footer=ab.Footer or"Chloee :3"
ab.Color=ab.Color or Color3.fromRGB(255,0,255)
ab["Tab Width"]=ab["Tab Width"]or 120
ab.Version=ab.Version or 1
ab.Keysystem=ab.Keysystem or{}

CURRENT_VERSION=ab.Version


