function aR.AddOption(a5,a6)
local a7,a8
if typeof(a6)=="table"and a6.Label and a6.Value~=nil then
a7=tostring(a6.Label)
a8=a6.Value
else
a7=tostring(a6)
a8=a6
end

local a9=Instance.new"Frame"
local ba=Instance.new"TextButton"
local bb=Instance.new"TextLabel"
local bc=Instance.new"Frame"
local bd=Instance.new"UIStroke"
local be=Instance.new"UICorner"
local bf=Instance.new"UICorner"

a9.BackgroundTransparency=1
a9.Size=UDim2.new(1,0,0,30)
a9.Name="Option"
a9.Parent=a2

bf.CornerRadius=UDim.new(0,3)
