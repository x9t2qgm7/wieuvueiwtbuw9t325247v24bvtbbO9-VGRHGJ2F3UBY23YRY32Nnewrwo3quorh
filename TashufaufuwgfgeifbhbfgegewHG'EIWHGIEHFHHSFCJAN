local function UpdateSizeScroll()
local aK=0
for aL,aM in _:GetChildren()do
if aM.Name~="UIListLayout"then
aK=aK+3+aM.Size.Y.Offset
end
end
_.CanvasSize=UDim2.new(0,0,0,aK)
end

local function UpdateSizeSection()
if aJ then
local aK=38
for aL,aM in aG:GetChildren()do
if aM.Name~="UIListLayout"and aM.Name~="UICorner"then
aK=aK+aM.Size.Y.Offset+3
end
end
h:Create(aD,TweenInfo.new(0.5),{Rotation=90}):Play()
h:Create(aw,TweenInfo.new(0.5),{Size=UDim2.new(1,1,0,aK)})
:Play()
h:Create(aG,TweenInfo.new(0.5),
{Size=UDim2.new(1,0,0,aK-38)}):Play()
h:Create(ax,TweenInfo.new(0.5),{Size=UDim2.new(1,0,0,2)}):Play()
task.wait(0.5)
UpdateSizeScroll()
end
end

if au==true then
aC:Destroy()
aD:Destroy()
aJ=true
UpdateSizeSection()
elseif au==false then
aJ=true
UpdateSizeSection()
else
aJ=false
end

if au~=true then
aC.Activated:Connect(function()
CircleClick(aC,j.X,j.Y)
if aJ then
h:Create(aD,TweenInfo.new(0.5),{Rotation=0}):Play()
h:Create(aw,TweenInfo.new(0.5),{Size=UDim2.new(1,1,0,30)}):Play()
h:Create(ax,TweenInfo.new(0.5),{Size=UDim2.new(0,0,0,2)})
:Play()
aJ=false
task.wait(0.5)
UpdateSizeScroll()
else
aJ=true
UpdateSizeSection()
end
end)
end

if au==true or au==false then
aJ=true
local aK=38
for aL,aM in aG:GetChildren()do
if aM.Name~="UIListLayout"and aM.Name~="UICorner"then
aK=aK+aM.Size.Y.Offset+3
end
end
aD.Rotation=90
aw.Size=UDim2.new(1,1,0,aK)
aG.Size=UDim2.new(1,0,0,aK-38)
ax.Size=UDim2.new(1,0,0,2)
UpdateSizeScroll()
end

aG.ChildAdded:Connect(UpdateSizeSection)
aG.ChildRemoved:Connect(UpdateSizeSection)

local aK=_:FindFirstChildOfClass"UIListLayout"
if aK then
aK:GetPropertyChangedSignal"AbsoluteContentSize":Connect(function()
_.CanvasSize=UDim2.new(0,0,0,aK.AbsoluteContentSize.Y+10)
end)
end

