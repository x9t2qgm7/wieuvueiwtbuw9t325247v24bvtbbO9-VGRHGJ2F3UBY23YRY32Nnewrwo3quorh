


local a=game:GetService"HttpService"

if not isfolder"Noctis"then
makefolder"Noctis"
end

local b=tostring(game:GetService"MarketplaceService":GetProductInfo(game.PlaceId).Name)
b=b:gsub("[^%w_ ]","")
b=b:gsub("%s+","_")

local c="Noctis/"..b
if not isfolder(c)then
makefolder(c)
end

local d
local e=c.."/autoload.txt"

ConfigData={}
Elements={}
CURRENT_VERSION=nil


function GetConfigList()
local f={}
if isfolder and listfiles then
local g=listfiles(c)
for h,i in pairs(g)do
local j=i:match"([^/\\]+)%.json$"
if j then
table.insert(f,j)
end
end
end
return f
end


function GetAutoload()
if isfile and isfile(e)then
local f,g=pcall(function()
return readfile(e)
end)
if f and g and g~=""then
return g:match"^%s*(.-)%s*$"
end
end
return nil
end


function SetAutoload(f)
if writefile then
writefile(e,f)
print("Autoload config set to: "..f)
end
end


function ClearAutoload()
if delfile and isfile(e)then
delfile(e)
print"Autoload config cleared"
end
end


function SaveConfigAs(f)
if not f or f==""then
warn"Config name cannot be empty"
return false
end

if writefile then
local g=c.."/"..f..".json"
ConfigData._version=CURRENT_VERSION
writefile(g,a:JSONEncode(ConfigData))
d=f
print("Config saved as: "..f)
return true
end
return false
end


function LoadConfig(f)
if not f or f==""then
warn"Config name cannot be empty"
return false
end

local g=c.."/"..f..".json"

if not CURRENT_VERSION then return false end
if isfile and isfile(g)then
local h,i=pcall(function()
return a:JSONDecode(readfile(g))
end)
if h and type(i)=="table"then
if i._version==CURRENT_VERSION then
ConfigData=i
d=f
LoadConfigElements()
print("Config loaded: "..f)
return true
else
warn"Config version mismatch"
return false
end
else
warn"Failed to decode config"
return false
end
else
warn("Config file not found: "..f)
return false
end
end


function DeleteConfig(f)
if not f or f==""then
warn"Config name cannot be empty"
return false
end

local g=c.."/"..f..".json"
if delfile and isfile(g)then
delfile(g)
print("Config deleted: "..f)


if GetAutoload()==f then
ClearAutoload()
end

return true
else
warn("Config file not found: "..f)
return false
end
end


function LoadConfigElements()
for f,g in pairs(Elements)do
if ConfigData[f]~=nil and g.Set then
g:Set(ConfigData[f],true)
end
end
end


function ResetElements()

ConfigData={_version=CURRENT_VERSION}


for f,g in pairs(Elements)do
if g.Set then
pcall(function()

if f:match"^Toggle_"then
g:Set(false)
elseif f:match"^Slider_"then
g:Set(20)
elseif f:match"^Input_"then
g:Set""
elseif f:match"^Dropdown_"then

if type(g.Value)=="table"then
g:Set{}
else
g:Set(nil)
end
end
end)
end
end

print"Elements reset to defaults"
end

