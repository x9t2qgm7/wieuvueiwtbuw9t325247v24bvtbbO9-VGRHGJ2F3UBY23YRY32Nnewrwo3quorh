end
local E=false
function r.Close(F)
if E then
return false
end
E=true
h:Create(
u,
TweenInfo.new(tonumber(q.Time),Enum.EasingStyle.Back,Enum.EasingDirection.InOut),
{Position=UDim2.new(0,400,0,0)}
):Play()
task.wait(tonumber(q.Time)/1.2)
t:Destroy()
end

B.Activated:Connect(function()
r:Close()
end)
h:Create(
u,
TweenInfo.new(tonumber(q.Time),Enum.EasingStyle.Back,Enum.EasingDirection.InOut),
{Position=UDim2.new(0,0,0,0)}
):Play()
task.wait(tonumber(q.Delay))
r:Close()
end)
return r
end

function chloex(o,p,q,r,s)
return n:MakeNotify{
Title=r or"Noctis",
Description=s or"Notification",
Content=o or"Content",
Color=q or Color3.fromRGB(138,43,226),
Delay=p or 4
}
end

function n.Window(o,p)
p=p or{}
p.Title=p.Title or"Chloe X"
p.Footer=p.Footer or"Chloee :3"
p.Color=p.Color or Color3.fromRGB(255,0,255)
p["Tab Width"]=p["Tab Width"]or 120
p.Version=p.Version or 1
p.Config=p.Config or"Default"

CURRENT_VERSION=p.Version

b=p.Config:gsub("%s+","_")
c="Noctis/"..b
e=c.."/autoload.txt"

if not isfolder(c)then
makefolder(c)
end

local q=GetAutoload()
if q then
LoadConfig(q)
else
ConfigData={_version=CURRENT_VERSION}
end

