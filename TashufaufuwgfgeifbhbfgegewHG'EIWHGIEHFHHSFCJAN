a0,
TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
{Size=UDim2.fromScale((a7-aP.Min)/(aP.Max-aP.Min),1)}
):Play()

aP.Callback(a7)
