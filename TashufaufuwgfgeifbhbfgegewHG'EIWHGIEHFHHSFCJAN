{Color=Color3.fromRGB(255,255,255),Transparency=0.9}):Play()
h:Create(aX,TweenInfo.new(0.2),
{BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=0.92}):Play()
end
end

aR:Set(aR.Value)
aM=aM+1
Elements[aQ]=aR
return aR
end

