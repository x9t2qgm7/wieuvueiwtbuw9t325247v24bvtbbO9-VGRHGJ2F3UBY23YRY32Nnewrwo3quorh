local g=game:GetService"UserInputService"
local h=game:GetService"TweenService"
local i=game:GetService"Players".LocalPlayer
local j=i:GetMouse()
local k=game:GetService"CoreGui"
local l=workspace.CurrentCamera.ViewportSize

local function isMobileDevice()
return g.TouchEnabled
and not g.KeyboardEnabled
and not g.MouseEnabled
end

local m=isMobileDevice()

local function safeSize(n,o)
local p=n/l.X
local q=o/l.Y

if m then
if p>0.5 then p=0.5 end
if q>0.3 then q=0.3 end
end

return UDim2.new(p,0,q,0)
end

local function MakeDraggable(n,o)
local function CustomPos(p,q)
local r,s,t,u

local function UpdatePos(v)
local w=v.Position-t
local x=UDim2.new(
u.X.Scale,
u.X.Offset+w.X,
u.Y.Scale,
u.Y.Offset+w.Y
)
local y=h:Create(q,TweenInfo.new(0.2),{Position=x})
y:Play()
end

p.InputBegan:Connect(function(v)
if v.UserInputType==Enum.UserInputType.MouseButton1 or v.UserInputType==Enum.UserInputType.Touch then
r=true
t=v.Position
u=q.Position
v.Changed:Connect(function()
if v.UserInputState==Enum.UserInputState.End then
r=false
end
end)
end
end)

p.InputChanged:Connect(function(v)
if v.UserInputType==Enum.UserInputType.MouseMovement or v.UserInputType==Enum.UserInputType.Touch then
s=v
end
end)

g.InputChanged:Connect(function(v)
if v==s and r then
UpdatePos(v)
end
end)
end

local function CustomSize(p)
local q,r,s,t

local u,v
local w,x

if m then
u,v=100,100
w,x=470,270
else
u,v=100,100
w,x=640,400
end

p.Size=UDim2.new(0,w,0,x)

local y=Instance.new"Frame"
y.AnchorPoint=Vector2.new(1,1)
y.BackgroundTransparency=1
y.Size=UDim2.new(0,40,0,40)
y.Position=UDim2.new(1,20,1,20)
y.Name="changesizeobject"
y.Parent=p

local function UpdateSize(z)
local A=z.Position-s
local B=t.X.Offset+A.X
local C=t.Y.Offset+A.Y

B=math.max(B,u)
C=math.max(C,v)

local D=h:Create(p,TweenInfo.new(0.2),{Size=UDim2.new(0,B,0,C)})
D:Play()
end

y.InputBegan:Connect(function(z)
if z.UserInputType==Enum.UserInputType.MouseButton1 or z.UserInputType==Enum.UserInputType.Touch then
q=true
s=z.Position
t=p.Size
z.Changed:Connect(function()
if z.UserInputState==Enum.UserInputState.End then
q=false
end
end)
end
end)

y.InputChanged:Connect(function(z)
if z.UserInputType==Enum.UserInputType.MouseMovement or z.UserInputType==Enum.UserInputType.Touch then
r=z
end
end)

g.InputChanged:Connect(function(z)
if z==r and q then
UpdateSize(z)
end
end)
end

CustomSize(o)
CustomPos(n,o)
end

function CircleClick(n,o,p)
spawn(function()
n.ClipsDescendants=true
local q=Instance.new"ImageLabel"
q.Image="rbxassetid://266543268"
q.ImageColor3=Color3.fromRGB(80,80,80)
q.ImageTransparency=0.8999999761581421
q.BackgroundColor3=Color3.fromRGB(255,255,255)
q.BackgroundTransparency=1
q.ZIndex=10
q.Name="Circle"
q.Parent=n

local r=o-q.AbsolutePosition.X
local s=p-q.AbsolutePosition.Y
q.Position=UDim2.new(0,r,0,s)
local t=0
if n.AbsoluteSize.X>n.AbsoluteSize.Y then
t=n.AbsoluteSize.X*1.5
elseif n.AbsoluteSize.X<n.AbsoluteSize.Y then
t=n.AbsoluteSize.Y*1.5
elseif n.AbsoluteSize.X==n.AbsoluteSize.Y then
t=n.AbsoluteSize.X*1.5
end

local u=0.5
q:TweenSizeAndPosition(UDim2.new(0,t,0,t),UDim2.new(0.5,-t/2,0.5,-t/2),"Out","Quad",
u,false,nil)
for v=1,10 do
q.ImageTransparency=q.ImageTransparency+0.01
wait(u/10)
end
q:Destroy()
end)
end

