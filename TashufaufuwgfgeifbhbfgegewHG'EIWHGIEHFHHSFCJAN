local O=false
local P,Q

local function update(R)
local S=R.Position-P
L.Position=UDim2.new(
Q.X.Scale,
Q.X.Offset+S.X,
Q.Y.Scale,
Q.Y.Offset+S.Y
)
end

N.InputBegan:Connect(function(R)
if R.UserInputType==Enum.UserInputType.MouseButton1 or R.UserInputType==Enum.UserInputType.Touch then
O=true
P=R.Position
Q=L.Position
R.Changed:Connect(function()
if R.UserInputState==Enum.UserInputState.End then
O=false
end
end)
end
end)

game:GetService"UserInputService".InputChanged:Connect(function(R)
if O and(R.UserInputType==Enum.UserInputType.MouseMovement or R.UserInputType==Enum.UserInputType.Touch)then
update(R)
end
end)
end

