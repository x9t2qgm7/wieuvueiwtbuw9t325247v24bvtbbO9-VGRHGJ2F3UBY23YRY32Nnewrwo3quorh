aO.SubButtonCallback=aO.SubCallback or aO.SubButtonCallback or
function()end

local aP="Panel_"..aO.Title
if ConfigData[aP]~=nil then
aO.Default=ConfigData[aP]
end

local aQ={Value=aO.Default}

local aR=50

if aO.Placeholder then
aR=aR+40
end

if aO.SubButtonText then
aR=aR+40
else
aR=aR+36
end

local aS=Instance.new"Frame"
