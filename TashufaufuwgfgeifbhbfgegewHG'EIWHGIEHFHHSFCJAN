u,
TweenInfo.new(tonumber(q.Time),Enum.EasingStyle.Back,Enum.EasingDirection.InOut),
{Position=UDim2.new(0,400,0,0)}
):Play()
task.wait(tonumber(q.Time)/1.2)
t:Destroy()
end

B.Activated:Connect(function()
r:Close()
end)
h:Create(
u,
TweenInfo.new(tonumber(q.Time),Enum.EasingStyle.Back,Enum.EasingDirection.InOut),
{Position=UDim2.new(0,0,0,0)}
):Play()
task.wait(tonumber(q.Delay))
r:Close()
end)
return r
end

function chloex(o,p,q,r,s)
return n:MakeNotify{
Title=r or"Noctis",
Description=s or"Notification",
Content=o or"Content",
Color=q or Color3.fromRGB(138,43,226),
Delay=p or 4
}
end




local function CreateKeySystem(o,p)
o=o or{}
o.Title=o.Title or"Key System"
o.Description=o.Description or"Enter your key below"
o.ScriptId=o.ScriptId or""
o.KeyFile=o.KeyFile or"NoctisPremium.txt"
o.Discord=o.Discord
o.GetKeyLinks=o.GetKeyLinks or{}
o.LogoAsset=o.LogoAsset or"rbxassetid://123156553209294"


local q,r=pcall(function()
return loadstring(game:HttpGet"https://sdkapi-public.luarmor.net/library.lua")()
end)

if not q then
warn("Failed to load Luarmor library:",r)
if p then p(false)end
return
end

r.script_id=o.ScriptId


local function checkKey(s)
local t,u=pcall(function()
return r.check_key(s)
end)

if t and u.code=="KEY_VALID"then
return true,u
end
return false,u
end

local function checkSavedKey()
local s


if getgenv().script_key then
s=getgenv().script_key
end


if not s then
s=rawget(_G,"script_key")or _G.script_key
end


if not s then
pcall(function()
if script_key and script_key~=""then
s=script_key
end
end)
end


if not s and isfile and readfile then
local t,u=pcall(function()
return readfile(o.KeyFile)
end)
if t and u and u~=""then
s=u:gsub("%s+","")
end
end

if s and s~=""then
s=s:gsub("%s+","")local
t=checkKey(s)

if t then

pcall(function()script_key=s end)
pcall(function()_G.script_key=s end)
pcall(function()getgenv().script_key=s end)


if writefile then
pcall(function()
writefile(o.KeyFile,s)
end)
end

return true,s
else

if isfile and delfile then
pcall(function()
if isfile(o.KeyFile)then
delfile(o.KeyFile)
end
end)
end
end
end

return false
end


local s,t=checkSavedKey()
if s then
if p then
task.spawn(p,true,t)
end
return
end


